{
    "$schema": "https://turborepo.org/schema.json",
    "pipeline": {
        "clean": {
            "dependsOn": ["^clean"],
            "outputs": ["dist/**", "lib/**"]
        },
        "build": {
            "dependsOn": ["^build"],
            "outputs": ["dist/**", "build/**", "lib/**"]
        },
        "dev": {
            "dependsOn": ["^dev"],
            "outputs": ["dist/**", "build/**", "lib/**"]
        },
        "lint": {
            "outputs": []
        },
        "format": {
            "dependsOn": ["^format"],
            "outputs": []
        },
        "test": {
            "dependsOn": ["^test"],
            "outputs": []
        },
        "manifest:version": {
            "dependsOn": ["^manifest:version"]
        }
    }
}
