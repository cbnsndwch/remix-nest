        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.1",
        "cross-env": "^7.0.3",
        "express": "^4.18.2",
        "isbot": "^3.7.0",
        "joi": "^17.12.1",
        "jwks-rsa": "^3.1.0",
        "lodash": "4.17.21",
        "mongoose": "^8.1.3",
        "passport": "^0.7.0",
        "passport-jwt": "^4.0.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "reflect-metadata": "^0.2.1",
        "rxjs": "^7.8.1",
        "source-map-support": "^0.5.21",
        "tiny-invariant": "^1.3.1"
