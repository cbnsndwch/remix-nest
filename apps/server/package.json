        "@nestjs/mongoose": "^10.0.4",
        "@nestjs/passport": "^10.0.3",
        "@nestjs/platform-express": "^10.3.3",
        "@nestjs/swagger": "^7.3.0",
        "@remix-run/express": "^2.6.0",
        "@remix-run/node": "^2.6.0",
        "@remix-run/react": "^2.6.0",
        "bcryptjs": "^2.4.3",
