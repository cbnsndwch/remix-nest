{
    "name": "@cbnsndwch/remix-nest-server",
    "version": "0.1.0",
    "description": "Remix/Vite/NestJS custom server demo",
    "private": true,
    "author": {
        "name": "Sergio Leon",
        "url": "https://cbnsndwch.io"
    },
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/cbnsndwch/remix-nest",
        "directory": "apps/server"
    },
    "scripts": {
        "build": "remix build && nest build",
        "dev": "nest start --watch",
        "debug": "nodemon src/main.ts",
        "dev:nest": "ts-node src/main.ts",
        "prod": "node dist/main",
        "format": "prettier --write src/**/*.ts",
        "lint": "eslint \"{src,app}/**/*.{ts,tsx}\" --fix",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json",
        "typecheck": "tsc"
    },
    "dependencies": {
        "@cbnsndwch/remix-nest-contracts": "workspace:*",
        "@nestjs/common": "^10.3.3",
        "@nestjs/config": "^3.2.0",
