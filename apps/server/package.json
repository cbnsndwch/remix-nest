        "eslint-plugin-react-hooks": "^4.6.0",
        "jest": "^29.7.0",
        "nodemon": "^3.0.3",
        "postcss": "^8.4.31",
        "prettier": "^3.2.5",
        "prettier-plugin-tailwindcss": "^0.5.6",
        "rimraf": "5.0.5",
        "source-map-support": "^0.5.21",
        "supertest": "^6.3.4",
        "tailwindcss": "^3.3.3",
